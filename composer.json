{
    "name": "sevenphp/sevenphp",
    "description": "High Speed Php Framework For Code Engineers",
    "type": "project",
    "version": "v0.9.9",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3.0",
        "firebase/php-jwt": "v5.2.0",
        "guzzlehttp/guzzle": "7.1.1",
        "nyholm/psr7": "^1.0@dev",
        "psr/http-message": "^1.0@dev",
        "psr/http-server-handler": "^1.0@dev",
        "sevens/model-trait": "^2.0.0",
        "sevens/router": "v3.1.0",
        "sevens/uploader-trait": "v1.0.0",
        "sevens/vars": "v2.0.0",
        "symfony/http-foundation": "^5.2@dev",
        "symfony/psr-http-message-bridge": "^2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Console\\": "app/Console/",
            "App\\Providers\\": "app/Providers/",
            "App\\Controllers\\": "app/Controllers/"
        },
        "files": [
                "app/Helpers/Helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "@php Engineer app::start"
        ]
    },
    "keywords": [ "SevenPHP", "API", "framework", "engineer" ]
}
